{
  "name": "@excalidraw/utils",
  "version": "0.1.0",
  "type": "module",
  "types": "./dist/types/utils/src/index.d.ts",
  "main": "./dist/prod/index.js",
  "module": "./dist/prod/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/utils/src/index.d.ts",
      "development": "./dist/dev/index.js",
      "production": "./dist/prod/index.js",
      "default": "./dist/prod/index.js"
    },
    "./*": {
      "types": "./dist/types/utils/src/*.d.ts"
    }
  },
  "files": [
    "dist/*"
  ],
  "description": "Excalidraw utility functions",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "excalidraw",
    "excalidraw-utils"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all",
      "not safari < 12",
      "not kaios <= 2.5",
      "not edge < 79",
      "not chrome < 70",
      "not and_uc < 13",
      "not samsung < 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@excalidraw/laser-pointer": "1.3.1",
    "browser-fs-access": "0.29.1",
    "open-color": "1.9.1",
    "png-chunk-text": "1.0.0",
    "png-chunks-encode": "1.0.0",
    "png-chunks-extract": "1.0.0"
  },
  "devDependencies": {
    "@nemati/typescript-config": "*",
    "cross-env": "7.0.3",
    "fonteditor-core": "2.4.0",
    "wawoff2": "2.0.1",
    "which": "4.0.0"
  },
  "peerDependencies": {
    "react": "^19.2.3"
  },
  "scripts": {
    "gen:types": "rimraf types && tsc",
    "build": "rimraf dist && node ../scripts/buildUtils.js && yarn gen:types"
  }
}
