{
  "extends": ["//"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "lint": {},
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": true
    },
    "dev:full": {
      "dependsOn": ["^dev:full"],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
